   CREATE TABLE TBL_BOOK(
         BOOK_NUMBER NUMBER,
         BOOK_TITLE VARCHAR2(100) NOT NULL,
         BOOK_AUTHOR VARCHAR2(100) NOT NULL,
         BOOK_RENT VARCHAR2(100) CONSTRAINT RENT_CHECK CHECK(BOOK_RENT = 'TRUE' OR BOOK_RENT = 'FALSE') NOT NULL,
         CONSTRAINT BOOK_NUMBER PRIMARY KEY(BOOK_NUMBER)
      );
